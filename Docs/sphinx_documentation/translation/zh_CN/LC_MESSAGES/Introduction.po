msgstr ""

#: ../../source/Introduction.rst:11 2ea44540f8c64ba7a84a3095ec52b4f9
msgid "1-, 2- and 3-D support"
msgstr ""

#: ../../source/Introduction.rst:12 2ae12d621e7b434785901b55bdf790d3
msgid "Support for cell-centered, face-centered, edge-centered, and nodal data"
msgstr ""

